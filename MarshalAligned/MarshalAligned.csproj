<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Authors>Nick Strupat</Authors>
    <Description>Aligned memory allocation methods in the style of `System.Runtime.InteropServices.Marshal`</Description>
    <PackageProjectUrl>https://github.com/NickStrupat/MarshalAligned</PackageProjectUrl>
    <PackageLicenseExpression>https://raw.githubusercontent.com/NickStrupat/MarshalAligned/master/LICENSE</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/NickStrupat/MarshalAligned</RepositoryUrl>
    <PackageTags>aligned alignment memory-allocation memory allocation allocator marshal alloc free</PackageTags>
  </PropertyGroup>

</Project>
